<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="id"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:id="@+id/dep_id"
        android:gravity="center_vertical|start"
        android:textColor="#000000"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_code"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_code"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_name"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_name"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_contact"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_contact"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_contactno"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_contactno"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_faxno"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_faxno"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_head"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_head"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_repname"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_repname"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dep_cp"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/dep_cp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="____________________________________________________"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000" />

</LinearLayout>
