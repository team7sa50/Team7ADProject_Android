<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="id"
            android:textSize="16dp"
            android:paddingLeft="5dp"
            android:id="@+id/sup_id"
            android:gravity="center_vertical|start"
            android:textColor="#000000"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="sup_code"
            android:textSize="16dp"
            android:paddingLeft="5dp"
            android:gravity="center_vertical|start"
            android:textColor="#000000"
            android:id="@+id/sup_code"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sup_name"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/sup_name"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sup_address"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/sup_address"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sup_contactno"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/sup_contactno"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sup_faxno"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000"
        android:id="@+id/sup_faxno"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="____________________________________________________"
        android:textSize="16dp"
        android:paddingLeft="5dp"
        android:gravity="center_vertical|start"
        android:textColor="#000000" />

    </LinearLayout>
